{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "wtjandra96/modern-react-client",
      "hostname": "client",
      "essential": false,
      "memory": 128
    },
    {
      "name": "server",
      "image": "wtjandra96/modern-node-server",
      "hostname": "api",
      "essential": false,
      "memory": 128
    },
    {
      "name": "nginx-proxy",
      "image": "wtjandra96/modern-nginx-proxy",
      "hostname": "nginx-proxy",
      "essential": true,
      "portMapping": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["client", "server"],
      "memory": 128
    }
  ]
}